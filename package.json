{
  "name": "performance-test-backend",
  "version": "1.0.0",
  "description": "",
  "author": "indigo online AG & AppsTeam",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:run": "node ./scripts/generate-orm-config.js && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "sh ./scripts/migration-revert.sh",
    "migration:generate": "sh ./scripts/migration-generate.sh",
    "migration:create": "sh ./scripts/migration-create.sh",
    "schema:drop": "sh ./scripts/schema-drop.sh",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "database:setup": "npm run schema:drop && npm run migration:run && npm run seed",
    "database:keep-alive": "ts-node ./scripts/db-keep-alive.ts"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.5.5",
    "@nestjs/platform-ws": "^7.5.5",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.4",
    "@nestjs/websockets": "^7.5.5",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "date-fns": "^2.16.1",
    "helmet": "^4.1.1",
    "i": "^0.3.6",
    "mysql": "^2.18.1",
    "nestjs-i18n": "^8.0.10",
    "nestjs-pino": "^1.2.0",
    "npm": "^6.14.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino-http": "^5.3.0",
    "pino-pretty": "^4.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "tunnel": "0.0.6",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pino": "^6.3.2",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pre-commit": [
    "format"
  ]
}
